  <rdfs:comment>An 'Activity' that is a part of an 'ActivityType'.</rdfs:comment>
  <rdfs:label>Constituent Activity</rdfs:label>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#inActivityType">
  <rdfs:domain rdf:resource="#Activity"/>
  <rdfs:range rdf:resource="#ActivityType"/>
  <rdfs:comment>The ActivityType containing the Activity.</rdfs:comment>
  <rdfs:label>Parent Activity Type</rdfs:label>
</owl:ObjectProperty>

